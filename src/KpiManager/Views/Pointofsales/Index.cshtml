@model IEnumerable<KpiManager.Pointofsale>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PointofsaleId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PointofsaleId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PointofsaleId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PointofsaleId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PointofsaleId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
